{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@service/*": ["src/service/*"],
      "@store/*": ["src/store/*"],
      "@icons/*": ["src/icons/*"],
      "@hooks/*": ["src/hooks/*"],
      "@helpers/*": ["src/helpers/*"],
      "@constant/*": ["src/constant/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
