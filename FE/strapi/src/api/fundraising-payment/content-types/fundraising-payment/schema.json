{
  "kind": "collectionType",
  "collectionName": "fundraising_payments",
  "info": {
    "singularName": "fundraising-payment",
    "pluralName": "fundraising-payments",
    "displayName": "FundraisingPayment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fundraisingId": {
      "type": "string",
      "regex": "^[^\\s/\\\\]+$",
      "required": true
    },
    "paymentApi": {
      "type": "string",
      "regex": "^[^\\s/\\\\]+$",
      "required": true
    },
    "total": {
      "type": "integer"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "uah",
        "eur",
        "usd"
      ],
      "default": "uah"
    },
    "userId": {
      "type": "integer"
    }
  }
}
